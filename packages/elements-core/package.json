{
  "name": "@stoplight/elements-core",
  "version": "7.14.0",
  "sideEffects": [
    "web-components.min.js",
    "src/web-components/**",
    "**/*.css"
  ],
  "homepage": "https://github.com/stoplightio/elements",
  "bugs": "https://github.com/stoplightio/elements/issues",
  "author": "Stoplight <support@stoplight.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/stoplightio/elements"
  },
  "license": "Apache-2.0",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "**/*"
  ],
  "engines": {
    "node": ">=14.13"
  },
  "scripts": {
    "build": "yarn build.react",
    "postbuild": "yarn build.styles",
    "build.react": "sl-scripts bundle",
    "build.styles": "postcss src/styles.css -o dist/styles.min.css && cp src/core.css dist",
    "build.docs": "storybook build -o dist-storybook",
    "commit": "git-cz",
    "release": "sl-scripts release",
    "release.docs": "sl-scripts release:docs",
    "release.dryRun": "sl-scripts release --dry-run --debug",
    "storybook": "storybook dev -p 9001",
    "test": "jest",
    "test.prod": "yarn test --coverage --maxWorkers=2",
    "test.update": "yarn test --updateSnapshot",
    "test.watch": "yarn test --watch",
    "test.packaging": "node --input-type=commonjs -e \"require('./dist/index.js')\" && node --input-type=module -e \"import './dist/index.mjs'\"",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "rollup": {
    "bundleDeps": [
      "@fortawesome/free-solid-svg-icons"
    ]
  },
  "dependencies": {
    "@stoplight/http-spec": "^6.0.2",
    "@stoplight/json": "^3.21.0",
    "@stoplight/json-schema-ref-parser": "^9.2.7",
    "@stoplight/json-schema-sampler": "0.3.0",
    "@stoplight/json-schema-tree": "^2.2.5",
    "@stoplight/json-schema-viewer": "^4.13.0",
    "@stoplight/markdown-viewer": "^5.6.0",
    "@stoplight/mosaic": "^1.45.0",
    "@stoplight/mosaic-code-editor": "^1.45.0",
    "@stoplight/mosaic-code-viewer": "^1.45.0",
    "@stoplight/path": "^2.0.0",
    "@stoplight/react-error-boundary": "^3.0.0",
    "@stoplight/types": "^14.0.0",
    "@stoplight/yaml": "^4.2.3",
    "classnames": "^2.3.2",
    "httpsnippet-lite": "^3.0.5",
    "jotai": "2.5.0",
    "json-schema": "^0.4.0",
    "lodash": "^4.17.19",
    "nanoid": "^5.0.3",
    "prop-types": "^15.7.2",
    "react-query": "^4.0.0",
    "react-router-dom": "^6.18.0",
    "react-router-hash-link": "^2.1.0",
    "tslib": "^2.6.2",
    "urijs": "^1.19.11",
    "util": "^0.12.5",
    "xml-formatter": "^3.6.0"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@stoplight/scripts": "9.2.1",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "3.x.x",
    "@types/enzyme-adapter-react-16": "1.x.x",
    "@types/json-schema": "^7.0.15",
    "@types/lodash": "^4.14.201",
    "@types/prop-types": "^15.7.10",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-hash-link": "^2.4.9",
    "@types/urijs": "^1.19.23",
    "enzyme": "3.x.x",
    "enzyme-adapter-react-16": "1.x.x",
    "enzyme-to-json": "3.x.x",
    "jest-enzyme": "^7.1.2",
    "jest-fetch-mock": "^3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "^18.2.0",
    "resolve-url-loader": "^5.0.0",
    "style-loader": "^3.3.3",
    "storybook": "next"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "release": {
    "extends": "@stoplight/scripts/release"
  }
}
